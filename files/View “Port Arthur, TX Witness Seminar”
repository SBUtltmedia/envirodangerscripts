	let player;  
   	let ytID;	
	function onYouTubeIframeAPIReady() {
              console.log("hehe")
        player = new YT.Player('player', {
          height: '390',
          width: '640',
          videoId: ytID || 'EvKNG0SXdGI',
          playerVars: {
            'playsinline': 1
          },
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        })
      }

      function onPlayerReady(event) {
        event.target.playVideo();
      }

      // 5. The API calls this function when the player's state changes.
      //    The function indicates that when playing a video (state=1),
      //    the player should play for six seconds and then stop.
      var done = false;
      function onPlayerStateChange(event) {
        if (event.data == YT.PlayerState.PLAYING && !done) {
          setTimeout(stopVideo, 6000);
          done = true;
        }
      }
      function stopVideo() {
        player.stopVideo();
      }

function playVideo(start){
//let iframe= document.getElementById("ytPlayer")
//iframe.setAttribute("src",url);
player.seekTo(start);
player.playVideo()
}


 function loadSeminarYT(id,startTimes){
      // 2. This code loads the IFrame Player API code asynchronously.
      var tag = document.createElement('script');
      tag.src = "https://www.youtube.com/iframe_api";
      var firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
for(i of startTimes){
let fragment = document.createDocumentFragment();
let a =Object.assign(document.createElement("a"),{href:`javascript:playVideo("${i.start}")`,innerHTML:i.description})
let page= document.getElementsByClassName("table")[0]
let curentDiv=Object.assign(document.createElement("div"),{className:"startLinks"})
page.append(curentDiv)

let insertDiv= document.getElementsByClassName("startLinks");
insertDiv =insertDiv[insertDiv.length-1]
let img =Object.assign(document.createElement("img"),{src:i.thumbnail})
fragment.append(img)
fragment.append(document.createElement("br"))
fragment.append(a)
fragment.append(document.createElement("br"))
insertDiv.append(fragment)
}      
      // 3. This function creates an <iframe> (and YouTube player)
      //    after the API code downloads.
      // 4. The API will call this function when the video player is ready.
 }	
